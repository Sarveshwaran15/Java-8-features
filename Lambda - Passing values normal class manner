interface addable{
    int addition(int a,int b);
}

class Sum implements addable{
    public int addition(int a,int b)
    {
        return (a+b);
    }
}
public class LambdaPassValues {
    public static void main(String[] args) {
        Sum sum=new Sum();
        int val=sum.addition(10,5);
        System.out.println(val);
    }
}
